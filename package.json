{
  "name": "@fxri/swagger2api",
  "version": "1.1.0",
  "description": "基于 swagger-typescript-api，将 Swagger 文档自动转为 TypeScript 格式的 API 文件。",
  "license": "MIT",
  "author": {
    "name": "唐启云",
    "email": "tqy@fxri.net",
    "url": "https://fxri.net:444/"
  },
  "type": "module",
  "main": "./dist/index.js",
  "bin": "./dist/bin/index.js",
  "files": [
    "dist",
    "README.md",
    ".swaggerrc",
    "package.json"
  ],
  "scripts": {
    "dev": "pnpm build:debug && node ./dist/bin/index.js",
    "build": "NODE_ENV=production tsup",
    "build:debug": "NODE_ENV=development tsup",
    "release": "pnpm build && pnpm publish",
    "api": "pnpm dev --axios --responses --remove-prefix-index 0"
  },
  "keywords": [],
  "packageManager": "pnpm@10.18.0",
  "dependencies": {
    "axios": "^1.12.2",
    "prompts": "^2.4.2",
    "swagger-typescript-api": "^13.2.15"
  },
  "devDependencies": {
    "@types/node": "^24.7.1",
    "@types/prompts": "^2.4.9",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-vue": "^9.33.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=20"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://github.com/fxri-net/swagger2api#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fxri-net/swagger2api.git"
  },
  "bugs": {
    "url": "https://github.com/fxri-net/swagger2api/issues"
  }
}
